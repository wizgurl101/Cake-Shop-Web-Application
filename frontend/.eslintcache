      <tr>\r\n                <th>PRODUCT ID</th>\r\n                <th>NAME</th>\r\n                <th>CATEGORY</th>\r\n                <th></th>\r\n              </tr>\r\n            </thead>\r\n            <tbody>\r\n              {products.map((product) => (\r\n                <tr key={product._id}>\r\n                  <td>{product._id}</td>\r\n                  <td>{product.name}</td>\r\n                  <td>{product.category}</td>\r\n                  <td>\r\n                    <LinkContainer to={`/admin/product/${product._id}/edit`}>\r\n                      <Button variant=\"light\" className=\"btn-sm\">\r\n                        <i className=\"fas fa-edit\" />\r\n                      </Button>\r\n                    </LinkContainer>\r\n                    <Button\r\n                      variant=\"danger\"\r\n                      className=\"btn-sm\"\r\n                      onClick={() =>\r\n                        deleteProductHander(product._id, product.name)\r\n                      }\r\n                    >\r\n                      <i className=\"fas fa-trash\" />\r\n                    </Button>\r\n                  </td>\r\n                </tr>\r\n              ))}\r\n            </tbody>\r\n          </Table>\r\n        </>\r\n      )}\r\n    </>\r\n  );\r\n};\r\n\r\nexport default ProductListScreen;\r\n","E:\\Cake-Shop-Web-Application\\frontend\\src\\components\\Header.js",[],"E:\\Cake-Shop-Web-Application\\frontend\\src\\components\\Loader.js",[],"E:\\Cake-Shop-Web-Application\\frontend\\src\\components\\Message.js",[],"E:\\Cake-Shop-Web-Application\\frontend\\src\\components\\FormContainer.js",[],"E:\\Cake-Shop-Web-Application\\frontend\\src\\actions\\productActions.js",[],"E:\\Cake-Shop-Web-Application\\frontend\\src\\actions\\userActions.js",[],"E:\\Cake-Shop-Web-Application\\frontend\\src\\constants\\productConstants.js",[],"E:\\Cake-Shop-Web-Application\\frontend\\src\\constants\\userConstants.js",[],"E:\\Cake-Shop-Web-Application\\